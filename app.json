{
  "expo": {
    "name": "Absensi SMK2 Trenggalek",
    "slug": "absensi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logosmk2.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logosmk2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.aitilokal.absensi",
      "supportsTablet": true
    },
    "android": {
      "package": "com.aitilokal.absensismk2trenggalek",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b8c92c64-e2d4-4538-9bb5-31af2c8ccce6"
      }
    }
  }
}
